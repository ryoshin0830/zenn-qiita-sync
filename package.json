    "express": "^4.18.2",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.12",
    "open": "^10.0.3",
    "ora": "^8.0.1",
    "zenn-cli": "^0.1.161"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
